    <profile>
      <id>thirdparty-uploads</id>
      <repositories>
        <repository>
          <id>thirdparty-uploads</id>
          <name>JBoss Third Party Uploads</name>
          <url>https://repository.jboss.org/nexus/content/repositories/thirdparty-uploads/</url>
          <layout>default</layout>
          <releases>
            <enabled>true</enabled>
            <updatePolicy>never</updatePolicy>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </repository>
      </repositories>
    </profile>
                        
  </profiles>
  

  <activeProfiles>
	<activeProfile>jdg-repository</activeProfile>  
    <activeProfile>jbip6</activeProfile>
    <activeProfile>jboss-techpreview</activeProfile>
    <activeProfile>jboss-public-repository</activeProfile>
    <activeProfile>jboss-developer-repository</activeProfile>
    <activeProfile>thirdparty-uploads</activeProfile>
       
  </activeProfiles>

</settings>
